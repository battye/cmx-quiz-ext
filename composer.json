    "homepage": "https://github.com/battye/ultimate-quiz-mod",
    "version": "3.0.0",
    "time": "2022-04-29",
    "license": "GPL-2.0-only",
    "authors": [
        {
            "name": "battye",
            "email": "battye@phpbb.com",
            "homepage": "https://forums.cricketmx.com",
            "role": "Lead Extension Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "phpbb/phpbb": ">=3.3.0@dev,<3.4.0@dev",
        "composer/installers": "~1.0.0"
